{
	"name": "iconkit-cli",
	"version": "0.2.2",
	"author": {
		"name": "Liquidiqq"
	},
	"homepage": "https://github.com/liquidiqq/iconkit-cli",
	"repository": {
		"type": "git",
		"url": "https://github.com/liquidiqq/iconkit-cli.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"icon",
		"icons",
		"svg",
		"community",
		"iconkit",
		"liquidiqq",
		"iconkit-cli",
		"iconkit cli"
	],
	"license": "MIT",
	"description": "Quickly setup Iconkit in Svelte/SvelteKit project",
	"main": "index.js",
	"bin": {
		"iconkit": "index.js"
	},
	"scripts": {
		"start": "node index.js",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
		"appendchangelog": "conventional-changelog -p angular -i CHANGELOG.md -s -a",
		"newchangelog": "conventional-changelog -p angular -o tmp-changelog.md",
		"mergechangelog": "cat tmp-changelog.md CHANGELOG.md > updated-changelog.md && mv updated-changelog.md CHANGELOG.md && rm tmp-changelog.md"
	},
	"type": "module",
	"dependencies": {
		"@clack/prompts": "^0.6.3",
		"kleur": "^4.1.5"
	}
}
